@model int[]

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Numbers</title>
</head>
<body>
    <main>
        <h1>Here are some numbers!</h1>
        <table>
            <tbody>
                @foreach (int num in Model)
                {
                    <tr>
                        <td>@num</td>
                    </tr>
                }
            </tbody>
        </table>
    </main>
</body>
</html>